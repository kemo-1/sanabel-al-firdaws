    "rehype-autolink-headings": "^7.1.0",
    "remark-custom-header-id": "file:packages/remark-custom-header-id",
    "remark-mark-highlight": "^0.1.1",
    "sharp": "^0.32.5",
    "starlight-blog": "^0.12.0",
    "starlight-view-modes": "file:packages/starlight-view-modes",
